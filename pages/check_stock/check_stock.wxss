.log-list {
  display: flex;
  flex-direction: column;
  margin: 20rpx;
}

.log-item {
  margin: 20rpx;
  display: flex;
}

.button_item {
  margin: 20rpx;
}

.button_item:active {
    background-color: rgb(68,50,41);
}

.input_item {
  margin: 20rpx;
  margin-top: 100rpx;
}

.btnGroup {
    display: flex;
    flex-direction: row;
    flex: 1;
    width: 100%;
    height: 100%;
    background-color: #fff;
    margin: 5rpx;
}

.item {
    display: flex;
    align-items: left;
    flex-direction: column;
    justify-content: left;
}
.orange {
    width:25%;
    color: #bfa69a;
    background: white;
    font-weight: bold;
    align-items: center;
}
.green {
    width:25%;
    color: white;
    background:  #bfa69a;
    font-weight: bold;
    align-items: center;
}
.blue {
    width:75%;
    color: white;
    background-color: #bfa69a;
    padding-left: 80rpx
}

.check_waiting {
    color: #bfa69a;
    background:  white;
    font-weight: bold;
    align-items: center;
}

